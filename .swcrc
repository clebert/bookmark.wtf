{
  "jsc": {
    "externalHelpers": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "target": "es2022",
    "transform": {
      "react": {
        "importSource": "preact",
        "runtime": "automatic"
      }
    }
  },
  "module": {
    "type": "es6"
  },
  "sourceMaps": true
}
