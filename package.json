{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "ci": "run-p compile:check format:check lint test",
    "compile:check": "tsc --pretty",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --silent",
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format:write",
    "predeploy": "npm run build:prod && cdk bootstrap --app 'npx aws-simple synth' && cdk diff --app 'npx aws-simple synth' --no-asset-metadata",
    "deploy": "cdk deploy --app 'npx aws-simple synth' --no-asset-metadata",
    "postdeploy": "aws-simple upload --yes && aws-simple redeploy --yes",
    "e2e-test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest-e2e-test.config.js",
    "start": "aws-simple start"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "@urql/core": "^3.2.2",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.6.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.5.0",
    "node-fetch": "^3.3.1",
    "preact": "^10.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@swc/core": "^1.3.41",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@types/aws-lambda": "^8.10.114",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.17",
    "@types/identicon.js": "^2.3.1",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "aws-cdk": "^2.69.0",
    "aws-simple": "^18.0.1",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "deepmerge": "^4.3.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.31.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "prettier": "^2.8.5",
    "resolve-typescript-plugin": "^2.0.1",
    "sonnar": "^1.0.0",
    "speakeasy": "^2.0.0",
    "start-server-and-test": "^2.0.0",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  }
}
