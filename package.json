{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "ci": "run-p format:test lint test",
    "codegen": "graphql-codegen --config codegen.yml",
    "predeploy": "npm run build:prod",
    "deploy": "cdk deploy --app 'npx aws-simple create'",
    "postdeploy": "aws-simple upload && aws-simple redeploy",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "preitest:dev": "fkill :3000 --silent && npm run build:dev && (npm start &)",
    "itest:dev": "npm run itest:prod",
    "postitest:dev": "fkill :3000 --silent",
    "itest:prod": "jest --config jest-itest.config.js",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "start": "aws-simple start",
    "test": "jest",
    "watch": "run-p start watch:*",
    "watch:build": "npm run build:dev -- --watch"
  },
  "dependencies": {
    "@clebert/bulma-preact": "^1.1.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@urql/core": "^1.16.1",
    "cookie": "^0.4.1",
    "graphql": "^15.4.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.2.0",
    "marked": "^1.2.7",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@types/aws-lambda": "^8.10.68",
    "@types/cookie": "^0.4.0",
    "@types/identicon.js": "^2.3.0",
    "@types/jest": "^26.0.19",
    "@types/marked": "^1.2.1",
    "@types/node-fetch": "^2.5.7",
    "@types/speakeasy": "^2.0.5",
    "aws-simple": "^4.2.1",
    "fkill-cli": "^6.1.0",
    "html-webpack-plugin": "next",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.7.1",
    "prettier": "^2.2.1",
    "speakeasy": "^2.0.0",
    "ts-loader": "^8.0.12",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0"
  }
}
