{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "ci": "run-p compile:check format:check lint test",
    "compile:check": "tsc --pretty",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "test": "jest --silent --passWithNoTests",
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format:write",
    "predeploy": "npm run build:prod && cdk bootstrap --app 'npx aws-simple synth' && cdk diff --app 'npx aws-simple synth' --no-asset-metadata",
    "deploy": "cdk deploy --app 'npx aws-simple synth' --no-asset-metadata",
    "postdeploy": "aws-simple upload --yes && aws-simple redeploy --yes",
    "e2e-test": "jest --config jest-e2e-test.config.js",
    "start": "node --enable-source-maps $(npm bin)/aws-simple start"
  },
  "dependencies": {
    "@urql/core": "^2.5.0",
    "cheerio": "^1.0.0-rc.11",
    "cookie": "^0.5.0",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.5.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.5.0",
    "node-fetch": "^3.2.10",
    "preact": "^10.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@swc/core": "^1.2.204",
    "@swc/helpers": "^0.4.2",
    "@swc/jest": "^0.2.21",
    "@types/aws-lambda": "^8.10.101",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.13",
    "@types/identicon.js": "^2.3.1",
    "@types/jest": "^28.1.3",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "autoprefixer": "^10.4.7",
    "aws-cdk": "^2.29.0",
    "aws-simple": "^17.1.2",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^2.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.22.2",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "resolve-typescript-plugin": "^1.2.0",
    "sonnar": "^0.7.0",
    "speakeasy": "^2.0.0",
    "start-server-and-test": "^1.14.0",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
