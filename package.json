{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "webpack --mode production",
    "ci": "run-p format:test lint test build",
    "codegen": "graphql-codegen --config codegen.yml",
    "deploy": "cdk deploy --app 'npx aws-simple create'",
    "postdeploy": "aws-simple upload && aws-simple redeploy",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "itest": "jest --config jest-itest.config.js",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "start": "aws-simple start",
    "test": "jest",
    "watch": "run-p start watch:*",
    "watch:build": "webpack --mode development --watch"
  },
  "dependencies": {
    "@clebert/bulma-preact": "^1.0.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@urql/core": "^1.16.0",
    "cookie": "^0.4.1",
    "graphql": "^15.4.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.1.2",
    "marked": "^1.2.5",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.7"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.76.0",
    "@aws-cdk/aws-certificatemanager": "^1.76.0",
    "@aws-cdk/aws-iam": "^1.76.0",
    "@aws-cdk/aws-lambda": "^1.76.0",
    "@aws-cdk/aws-route53": "^1.76.0",
    "@aws-cdk/aws-route53-targets": "^1.76.0",
    "@aws-cdk/aws-s3": "^1.76.0",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@types/aws-lambda": "^8.10.64",
    "@types/cookie": "^0.4.0",
    "@types/identicon.js": "^2.3.0",
    "@types/jest": "^26.0.16",
    "@types/marked": "^1.2.0",
    "@types/node-fetch": "^2.5.7",
    "@types/speakeasy": "^2.0.5",
    "aws-cdk": "^1.76.0",
    "aws-simple": "^4.1.3",
    "html-webpack-plugin": "next",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.6.2",
    "prettier": "^2.2.1",
    "speakeasy": "^2.0.0",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  }
}
