{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "ci": "run-p compile format:check lint test",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format",
    "compile": "tsc --pretty",
    "predeploy": "npm run build:prod && cdk bootstrap --app 'npx aws-simple synth' && cdk diff --app 'npx aws-simple synth' --no-asset-metadata",
    "deploy": "cdk deploy --app 'npx aws-simple synth' --no-asset-metadata",
    "postdeploy": "aws-simple upload --yes && aws-simple redeploy --yes",
    "e2e-test": "jest --config jest-e2e-test.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "prepare": "run-s setup",
    "setup": "node onecfg.mjs",
    "prestart": "fkill :3000 --silent && echo $(npm bin)/aws-simple",
    "start": "node --enable-source-maps $(npm bin)/aws-simple start",
    "test": "jest --silent"
  },
  "dependencies": {
    "@urql/core": "^2.4.3",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.2",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.3.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.5.0",
    "node-fetch": "^3.2.1",
    "preact": "^10.6.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@onecfg/standard": "^2.2.0",
    "@swc/core": "^1.2.148",
    "@swc/helpers": "^0.3.6",
    "@swc/jest": "^0.2.20",
    "@types/aws-lambda": "^8.10.93",
    "@types/cookie": "^0.4.1",
    "@types/identicon.js": "^2.3.1",
    "@types/jest": "^27.4.1",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "aws-cdk": "^2.15.0",
    "aws-simple": "^16.0.0",
    "css-loader": "^6.7.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-markdown": "^2.2.1",
    "fkill-cli": "^7.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.19.2",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sonnar": "^0.7.0",
    "speakeasy": "^2.0.0",
    "swc-loader": "^0.1.15",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
