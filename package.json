{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "ci": "run-p compile format:check lint test",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format",
    "compile": "onecmd compile",
    "predeploy": "npm run build:prod && cdk bootstrap --app 'npx aws-simple synthesize'&& cdk diff --app 'npx aws-simple synthesize'",
    "deploy": "cdk deploy --app 'npx aws-simple synthesize'",
    "postdeploy": "aws-simple upload --yes && aws-simple redeploy --yes",
    "e2e-test": "jest --config jest-e2e-test.config.js",
    "format": "onecmd format",
    "format:check": "onecmd format --check",
    "lint": "onecmd lint",
    "prepare": "run-s setup",
    "setup": "onecmd setup",
    "prestart": "fkill :3000 --silent",
    "start": "aws-simple start",
    "test": "onecmd test"
  },
  "dependencies": {
    "@urql/core": "^2.3.6",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.1",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.3.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.5.0",
    "node-fetch": "^3.2.0",
    "preact": "^10.6.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@onecmd/standard-plugins": "^0.18.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/cookie": "^0.4.1",
    "@types/identicon.js": "^2.3.1",
    "@types/speakeasy": "^2.0.7",
    "autoprefixer": "^10.4.2",
    "aws-cdk": "^2.12.0",
    "aws-simple": "^13.0.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "deepmerge": "^4.2.2",
    "fkill-cli": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.19.0",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "sonnar": "^0.7.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^3.0.17",
    "ts-loader": "^9.2.6",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  }
}
