{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "ci": "run-p compile format:test lint test",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format",
    "compile": "tsc --project tsconfig.json --pretty --noEmit",
    "predeploy": "npm run build:prod",
    "deploy": "cdk deploy --app 'npx aws-simple create'",
    "postdeploy": "aws-simple upload && aws-simple redeploy",
    "e2e-test": "jest --config jest-e2e-test.config.js",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "prestart": "fkill :3000 --silent",
    "start": "aws-simple start",
    "test": "jest",
    "watch": "run-p start watch:*",
    "watch:build": "npm run build:dev -- --watch"
  },
  "dependencies": {
    "@urql/core": "^2.0.0",
    "cookie": "^0.4.1",
    "graphql": "^15.5.0",
    "identicon.js": "^2.3.3",
    "loxia": "^0.5.0",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@types/aws-lambda": "^8.10.72",
    "@types/cookie": "^0.4.0",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/identicon.js": "^2.3.0",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node-fetch": "^2.5.8",
    "@types/speakeasy": "^2.0.5",
    "autoprefixer": "^10.2.4",
    "aws-simple": "^5.0.0",
    "css-loader": "^5.0.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "fkill-cli": "^6.1.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.8",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.9.0",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "sonnar": "^0.7.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^2.0.3",
    "ts-loader": "^8.0.17",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0"
  }
}
