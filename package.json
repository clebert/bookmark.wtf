{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "ci": "run-p compile:check format:check lint test",
    "compile:check": "tsc --pretty",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --silent",
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "postbuild:prod": "size-limit",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format:write",
    "predeploy": "npm run build:prod && cdk bootstrap --app 'npx aws-simple synth' && cdk diff --app 'npx aws-simple synth' --no-asset-metadata",
    "deploy": "cdk deploy --app 'npx aws-simple synth' --no-asset-metadata",
    "postdeploy": "aws-simple upload --yes && aws-simple redeploy --yes",
    "e2e-test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest-e2e-test.config.js",
    "start": "aws-simple start"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "@urql/core": "^4.0.10",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "fast-deep-equal": "^3.1.3",
    "identicon.js": "^2.3.3",
    "loxia": "^1.0.1",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@size-limit/preset-app": "^8.2.4",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@types/aws-lambda": "^8.10.116",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.17",
    "@types/identicon.js": "^2.3.1",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "aws-cdk": "^2.82.0",
    "aws-simple": "^18.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.34.3",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.2",
    "prettier": "^2.8.8",
    "resolve-typescript-plugin": "^2.0.1",
    "size-limit": "^8.2.4",
    "sonnar": "^1.0.0",
    "speakeasy": "^2.0.0",
    "start-server-and-test": "^2.0.0",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3"
  },
  "size-limit": [
    {
      "name": "brotli compression",
      "path": "./dist/app/index.*.js",
      "brotli": true
    },
    {
      "name": "gzip compression",
      "path": "./dist/app/index.*.js",
      "gzip": true
    },
    {
      "name": "no compression",
      "path": "./dist/app/index.*.js",
      "gzip": false
    }
  ]
}
