{
  "name": "bookmark.wtf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode development",
    "prebuild:prod": "rm -rf dist",
    "build:prod": "webpack --mode production",
    "ci": "run-p compile format:check lint test",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run format",
    "compile": "onecmd compile",
    "predeploy": "npm run build:prod && cdk bootstrap --app 'npx aws-simple create'",
    "deploy": "cdk deploy --app 'npx aws-simple create'",
    "postdeploy": "aws-simple upload && aws-simple redeploy",
    "e2e-test": "jest --config jest-e2e-test.config.js",
    "format": "onecmd format",
    "format:check": "onecmd format --check",
    "lint": "onecmd lint",
    "prepare": "run-s setup",
    "setup": "onecmd setup",
    "prestart": "fkill :3000 --silent",
    "start": "aws-simple start",
    "test": "onecmd test"
  },
  "dependencies": {
    "@urql/core": "^2.3.1",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.1",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^15.5.3",
    "identicon.js": "^2.3.3",
    "loxia": "^0.5.0",
    "node-fetch": "^2.6.2",
    "preact": "^10.5.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^2.1.3",
    "@onecmd/standard-plugins": "^0.11.1",
    "@types/aws-lambda": "^8.10.83",
    "@types/cookie": "^0.4.1",
    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/identicon.js": "^2.3.1",
    "@types/mini-css-extract-plugin": "^2.2.0",
    "@types/node-fetch": "^2.5.12",
    "@types/speakeasy": "^2.0.6",
    "autoprefixer": "^10.3.4",
    "aws-cdk": "next",
    "aws-simple": "^9.6.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "deepmerge": "^4.2.2",
    "fkill-cli": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.14.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "sonnar": "^0.7.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^2.2.14",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  }
}
